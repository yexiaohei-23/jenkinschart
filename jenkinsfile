pipeline{
    agent any
    stages{
        stage('build'){
            steps{
                echo 'hello world'
                
            }
        stage('make'){
            steps{
                echo 'make'
                
            }
        }
        stage('test'){
            steps{
                echo 'test'
            }
        }
        stage('deployment'){
            steps{
                echo 'deployment'
            }
        }
        }
    }
}
